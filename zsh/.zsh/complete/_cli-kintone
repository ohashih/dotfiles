#compdef cli-kintone
###-begin-cli-kintone-completions-###
#
# yargs command completion script
#
# Installation: /usr/local/bin/cli-kintone completion >> ~/.zshrc
#    or /usr/local/bin/cli-kintone completion >> ~/.zprofile on OSX.
#
_cli-kintone_yargs_completions()
{
  local reply
  local si=$IFS
  IFS=$'
' reply=($(COMP_CWORD="$((CURRENT-1))" COMP_LINE="$BUFFER" COMP_POINT="$CURSOR" /usr/local/bin/cli-kintone --get-yargs-completions "${words[@]}"))
  IFS=$si
  _describe 'values' reply
}
compdef _cli-kintone_yargs_completions cli-kintone
###-end-cli-kintone-completions-###

